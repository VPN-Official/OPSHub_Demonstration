  "kpis": [
    {
      "id": "mttr",
      "name": "Mean Time To Resolve",
      "description": "Average time to resolve incidents",
      "type": "time",
      "unit": "minutes",
      "target_value": 240,
      "target_operator": "lt",
      "calculation_method": "avg(resolved_at - created_at)",
      "data_source": "incidents",
      "refresh_interval": 15,
      "enabled": true
    },
    {
      "id": "uptime",
      "name": "Service Uptime",
      "description": "Percentage of time services are available",
      "type": "percentage",
      "unit": "%",
      "target_value": 99.9,
      "target_operator": "gte",
      "calculation_method": "uptime_seconds / total_seconds * 100",
      "data_source": "monitoring_system",
      "refresh_interval": 5,
      "enabled": true
    },
    {
      "id": "response_time",
      "name": "Response Time",
      "description": "Average API response time",
      "type": "time",
      "unit": "ms",
      "target_value": 200,
      "target_operator": "lt",
      "calculation_method": "sum(response_times) / count(requests)",
      "data_source": "apm_system",
      "refresh_interval": 5,
      "enabled": true
    },
    {
      "id": "error_rate",
      "name": "Error Rate",
      "description": "Percentage of requests resulting in errors",
      "type": "quality",
      "unit": "%",
      "target_value": 1.0,
      "target_operator": "lt",
      "calculation_method": "error_count / total_requests * 100",
      "data_source": "logging_system",
      "refresh_interval": 5,
      "enabled": true
    },
    {
      "id": "sla_compliance",
      "name": "SLA Compliance Rate",
      "description": "Percentage of SLAs met",
      "type": "compliance",
      "unit": "%",
      "target_value": 95.0,
      "target_operator": "gte",
      "calculation_method": "slas_met / total_slas * 100",
      "data_source": "sla_tracking",
      "refresh_interval": 60,
      "enabled": true
    }
  ],
  "automation": [
    {
      "id": "auto_incident_triage",
      "name": "Automated Incident Triage",
      "description": "Automatically categorize and assign incidents based on keywords and patterns",
      "trigger_conditions": {
        "entity_type": "incidents",
        "status": "new",
        "has_keywords": ["database", "network", "application"]
      },
      "actions": [
        {
          "type": "categorize",
          "parameters": {
            "category_mapping": {
              "database": "infrastructure",
              "network": "network",
              "application": "application"
            }
          }
        },
        {
          "type": "assign",
          "parameters": {
            "assignment_rules": {
              "infrastructure": "db_team",
              "network": "network_team",
              "application": "app_team"
            }
          }
        }
      ],
      "enabled": true
    },
    {
      "id": "sla_breach_notification",
      "name": "SLA Breach Notification",
      "description": "Send notifications when SLA thresholds are breached",
      "trigger_conditions": {
        "sla_status": "warning",
        "threshold_percentage": 80
      },
      "actions": [
        {
          "type": "notify",
          "parameters": {
            "channels": ["email", "slack"],
            "recipients": ["manager", "stakeholders"],
            "template": "sla_breach_warning"
          }
        }
      ],
      "enabled": true
    }
  ],
  "ai_models": [
    {
      "id": "incident_classifier",
      "name": "Incident Classification",
      "description": "Automatically classify incidents by category and priority",
      "type": "classification",
      "model_config": {
        "model_type": "text_classification",
        "features": ["title", "description", "affected_systems"],
        "algorithm": "transformer",
        "model_path": "/models/incident_classifier_v1.pkl"
      },
      "triggers": ["incident_created", "incident_updated"],
      "confidence_threshold": 0.8,
      "enabled": true
    },
    {
      "id": "anomaly_detector",
      "name": "System Anomaly Detection",
      "description": "Detect anomalies in system metrics and performance data",
      "type": "anomaly_detection",
      "model_config": {
        "model_type": "isolation_forest",
        "features": ["cpu_usage", "memory_usage", "disk_io", "network_io"],
        "algorithm": "isolation_forest",
        "contamination": 0.1
      },
      "triggers": ["metric_data_received"],
      "confidence_threshold": 0.7,
      "enabled": true
    },
    {
      "id": "root_cause_analyzer",
      "name": "Root Cause Analysis",
      "description": "Analyze incidents and suggest potential root causes",
      "type": "root_cause_analysis",
      "model_config": {
        "model_type": "correlation_analysis",
        "features": ["incident_symptoms", "system_logs", "configuration_changes", "deployment_history"],
        "algorithm": "gradient_boosting",
        "lookback_days": 30
      },
      "triggers": ["problem_created"],
      "confidence_threshold": 0.6,
      "enabled": true
    },
    {
      "id": "change_risk_assessor",
      "name": "Change Risk Assessment",
      "description": "Evaluates risk level of proposed changes",
      "type": "predictive",
      "model_config": {
        "model_type": "risk_assessment",
        "features": ["change_type", "affected_systems", "deployment_method", "historical_failures"],
        "algorithm": "neural_network"
      },
      "triggers": ["change_request_submitted"],
      "confidence_threshold": 0.75,
      "enabled": true
    }
  ],
  "tenant_settings": {
    "timezone": "UTC",
    "business_hours": {
      "start": "09:00",
      "end": "17:00",
      "days": [1, 2, 3, 4, 5]
    },
    "notification_settings": {
      "email_enabled": true,
      "sms_enabled": true,
      "slack_enabled": true,
      "default_channels": ["email", "slack"]
    },
    "compliance_frameworks": ["SOX", "PCI-DSS", "HIPAA", "GDPR", "ISO27001"]
  },
  "validation": {
    "required_fields": {
      "incidents": ["title", "description", "priority", "status"],
      "service_requests": ["title", "description", "priority", "requested_by"],
      "change_requests": ["title", "description", "type", "risk_level", "implementation_date"],
      "problems": ["title", "description", "priority", "category"],
      "assets": ["name", "type", "owner", "tier"]
    },
    "field_constraints": {
      "incidents": {
        "title": {
          "type": "string",
          "min_length": 5,
          "max_length": 200
        },
        "description": {
          "type": "string",
          "min_length": 10,
          "max_length": 5000
        },
        "priority": {
          "type": "enum",
          "enum_values": ["P1", "P2", "P3", "P4", "critical", "high", "medium", "low"]
        },
        "status": {
          "type": "enum",
          "enum_values": ["new", "in_progress", "resolved", "closed", "cancelled"]
        }
      },
      "service_requests": {
        "title": {
          "type": "string",
          "min_length": 5,
          "max_length": 200
        },
        "priority": {
          "type": "enum", 
          "enum_values": ["low", "medium", "high", "emergency", "critical"]
        }
      },
      "problems": {
        "priority": {
          "type": "enum",
          "enum_values": ["P1", "P2", "P3", "P4", "critical", "high", "medium", "low"]
        },
        "status": {
          "type": "enum",
          "enum_values": ["new", "in_analysis", "analysis", "known_error", "root_cause_identified", "mitigated", "closed"]
        }
      }
    }
  }
}